#include "token.h"

Token::Token(){
  type = 0;
  lexeme = "";
  line = 0;
  pos = 0;
}

Token::Token(int t,string l,int n,int p){
  type = t;
  lexeme = l;
  line = n;
  pos = p;
}

Token::~Token(){
}

int Token::getType(){
  return type;
}

string Token::getLexeme(){
  return lexeme;
}

int Token::getLine(){
  return line;
}

int Token::getPos(){
  return pos;
}

